{
    "name": "@deciosfernandes/mapbox-v3-gl-style-switcher",
    "version": "1.2.1",
    "description": "A modern, fully-typed TypeScript control for Mapbox GL JS with dropdown-style interface for switching between map styles",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./styles.css": "./styles.css"
    },
    "files": [
        "dist",
        "styles.css"
    ],
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "vitest run"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/deciosfernandes/style-switcher.git"
    },
    "keywords": [
        "mapbox",
        "mapbox-gl-js",
        "style-switcher",
        "typescript",
        "map-control",
        "style-control",
        "dropdown",
        "ui-component"
    ],
    "author": "Decio Fernandes",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/deciosfernandes/style-switcher/issues"
    },
    "homepage": "https://github.com/deciosfernandes/style-switcher#readme",
    "peerDependencies": {
        "mapbox-gl": "^3.x"
    },
    "devDependencies": {
        "@types/mapbox-gl": "^3.4.1",
        "jsdom": "^26.0.0",
        "mapbox-gl": "^3.14.0",
        "typescript": "^5.9.3",
        "vitest": "^3.0.0"
    }
}
